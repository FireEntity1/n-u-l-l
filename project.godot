; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="null"
run/main_scene="res://levels/title.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Global="*res://scripts/global.gd"

[dialogic]

directories/dch_directory={
"???": "res://dialogic/???.dch",
"NORA": "res://dialogic/NORA.dch",
"SYSTEM": "res://dialogic/SYSTEM.dch",
"data_log": "res://dialogic/data_log.dch"
}
directories/dtl_directory={
"level1": "res://dialogic/level1.dtl",
"level1end": "res://dialogic/level1end.dtl",
"level2end": "res://dialogic/level2end.dtl",
"level2glitch": "res://dialogic/level2glitch.dtl",
"level2jump": "res://dialogic/level2jump.dtl",
"level2jumppad": "res://dialogic/level2jumppad.dtl",
"level2main": "res://dialogic/level2main.dtl",
"level2start": "res://dialogic/level2start.dtl",
"level3": "res://dialogic/level3.dtl",
"level3drop": "res://dialogic/level3drop.dtl",
"level3postdrop": "res://dialogic/level3postdrop.dtl",
"level3system": "res://dialogic/level3system.dtl",
"level4???interaction": "res://dialogic/level4???interaction.dtl",
"level4end": "res://dialogic/level4end.dtl",
"level4intro": "res://dialogic/level4intro.dtl",
"level5loredrop": "res://dialogic/level5loredrop.dtl",
"level6confrontation": "res://dialogic/level6confrontation.dtl",
"level7bossintro": "res://dialogic/level7bossintro.dtl",
"level7death": "res://dialogic/level7death.dtl"
}
variables={}
layout/style_list=["res://dialogic/style.tres"]
layout/default_style="res://dialogic/style.tres"
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
audio/max_channels=4.0
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
text/autoadvance_enabled=false
glossary/default_case_sensitive=true
text/autoadvance_fixed_delay=1.0
text/autoadvance_per_word_delay=0
timer/process_in_physics=true
text/autoskip_time_per_event=1.0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/godot_super-wakatime/plugin.cfg")

